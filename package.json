{
  "name": "swiper-action",
  "version": "0.1.2",
  "description": "simple component for swipe-to-action",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && parcel build src/index.tsx",
    "dev": "parcel watch src/index.tsx",
    "prepack": "npm run build",
    "semantic-release": "semantic-release",
    "test": "echo \" no tests are currently setup\""
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cuvar/swiper-action.git"
  },
  "keywords": [
    "npm",
    "javascript",
    "typescript",
    "react",
    "swipe",
    "action"
  ],
  "author": "cuvar <luca@cuvar.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cuvar/swiper-action/issues"
  },
  "homepage": "https://github.com/cuvar/swiper-action#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "autoprefixer": "^10.4.13",
    "parcel": "^2.8.3",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "semantic-release": "^19.0.3",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "jotai": "^1.13.1"
  }
}
